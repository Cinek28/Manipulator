  </link>

  <joint
    name="wrist_pitch"
    type="revolute">
    <origin xyz="0 0 0.129" rpy="-1.5708 0 1.603" />
    <parent link="wrist_rotation_link" />
    <child link="wrist_pitch_link" />
    <axis xyz="0 0 -1" />
    <limit
      lower="0"
      upper="3.14"
      effort="10.8"
