      </actuator>
  </transmission>

  <link name="wrist_pitch_link">
    <inertial>
      <origin xyz="0.02283 1.6924E-05 4.4001E-05" rpy="0 0 0" />
      <mass value="0.22279" />
      <inertia
        ixx="8.4052E-05"
        ixy="8.7929E-08"
        ixz="-3.0476E-07"
        iyy="0.00016875"
        iyz="4.7854E-08"
        izz="8.4698E-05" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
